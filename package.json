{
  "name": "dark-side",
  "description": "Light and dark theme design plugin for Sketch",
  "version": "1.0.2",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Dark Side",
    "manifest": "src/manifest.json",
    "main": "darkside.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3"
  },
  "author": "receptiryaki <recep.tiryaki@gmail.com>",
  "dependencies": {}
}
